# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,159,98)
  Font = Font["Nimbus Mono L"]
  Icon = Picture["cascoimperial.png"]
  Arrangement = Arrange.Vertical
  { DrawingArea1 DrawingArea
    MoveScaled(1,1,55,53)
    Expand = True
    AutoResize = True
    Border = Border.Plain
  }
  { TextAreaInformacion TextArea
    MoveScaled(7,81,77,12)
    Font = Font["Bold,+2"]
    Background = &H000000&
    Foreground = &HFFFFFF&
    ReadOnly = True
    Wrap = True
  }
  { TimerActualizarPosiciones #Timer
    #MoveScaled(61,51)
    Delay = 500
  }
  { TimerProduccion #Timer
    #MoveScaled(74,51)
    Delay = 50000
  }
  { TimerFlotasEnemigas #Timer
    #MoveScaled(89,51)
    Delay = 25000
  }
}
